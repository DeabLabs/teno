{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": ["NODE_ENV"],
	"globalDependencies": [".env"],
	"pipeline": {
		"start": {
			"persistent": true,
			"dependsOn": ["build"]
		},
		"build": {
			"dependsOn": ["^build", "db:generate"],
			"outputs": ["dist/**", "build/**", "public/build/**"]
		},
		"db:generate": {
			"cache": false
		},
		"dev": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build"]
		},
		"db:push": {
			"cache": false
		},
		"db:seed": {
			"cache": false
		},
		"test": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"test:w": {
			"cache": false,
			"persistent": true,
			"dependsOn": ["^build"]
		},
		"lint": {
			"cache": false,
			"dependsOn": ["^build"]
		},
		"clean": {
			"cache": false
		}
	}
}
