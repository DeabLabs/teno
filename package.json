{
	"name": "teno",
	"version": "0.0.1",
	"private": true,
	"description": "AI Meeting Manager",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"scripts": {
		"start": "turbo run start --no-daemon",
		"dev": "turbo run dev --no-daemon",
		"build": "turbo run build --no-daemon",
		"lint": "turbo run lint --no-daemon",
		"test": "turbo run test --no-daemon",
		"test:w": "turbo run test:w --no-daemon",
		"clean": "turbo run clean --no-daemon",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"ci": "turbo run lint test build --no-daemon",
		"fix-deps": "manypkg fix",
		"postinstall": "manypkg check",
		"db:push": "turbo run db:push --no-daemon",
		"db:seed": "turbo run db:seed --no-daemon",
		"#docker commands#": "",
		"d-up": "docker-compose up -d",
		"d-down": "docker-compose stop"
	},
	"prisma": {
		"schema": "packages/database/prisma/schema.prisma"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/cephalization/infinite-mystery.git"
	},
	"license": "MIT",
	"dependencies": {
		"@manypkg/cli": "0.20.0",
		"prettier": "2.8.4",
		"turbo": "1.7.4",
		"typescript": "^5.0.3"
	},
	"engines": {
		"node": "18"
	},
	"packageManager": "yarn@1.22.19"
}
