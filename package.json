{
	"name": "teno-bot",
	"version": "0.0.1",
	"description": "Discord Stenographer bot written using @discordjs/voice",
	"exports": "./dist/bot.js",
	"type": "module",
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"start": "yarn build && node dist/bot",
		"dev": "tsup --watch --onSuccess \"node dist/bot.js\"",
		"lint": "prettier --cache --check . && eslint src --ext mjs,js,ts --cache",
		"format": "prettier --cache --write . && eslint src --ext mjs,js,ts --fix --cache",
		"build": "npm run db:generate && tsup",
		"build:check": "tsc --noEmit --incremental false",
		"ci": "yarn build:check && yarn lint",
		"#docker commands#": "",
		"d-up": "docker-compose up -d",
		"d-down": "docker-compose down",
		"#db commands#": "",
		"db:push": "prisma db push",
		"db:generate": "prisma generate",
		"db:studio": "prisma studio"
	},
	"author": "Blindmansion <blindmansion@gmail.com>",
	"license": "MIT",
	"dependencies": {
		"@deepgram/sdk": "^1.18.3",
		"@discordjs/opus": "^0.8.0",
		"@discordjs/voice": "^0.11.0",
		"@google-cloud/text-to-speech": "^4.2.1",
		"@prisma/client": "^4.12.0",
		"discord.js": "^14.8.0",
		"dotenv": "^16.0.3",
		"langchain": "^0.0.40",
		"libsodium-wrappers": "^0.7.10",
		"node-crc": "^1.3.2",
		"node-fetch": "^3.3.1",
		"prism-media": "^2.0.0-alpha.0",
		"redis": "^4.6.5",
		"tiny-invariant": "^1.3.1",
		"znv": "0.3.2",
		"zod": "3.20.6"
	},
	"devDependencies": {
		"@dword-design/eslint-plugin-import-alias": "^3.0.4",
		"@types/node": "^18.15.10",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.23.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-typescript": "^3.5.0",
		"eslint-plugin-import": "^2.26.0",
		"prettier": "^2.7.1",
		"prisma": "^4.12.0",
		"rimraf": "^3.0.2",
		"tsup": "^6.7.0",
		"typescript": "^4.8.2"
	},
	"packageManager": "yarn@1.22.19"
}
